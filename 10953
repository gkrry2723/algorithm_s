public class Main {

	public static void main(String[] args) {
		int T;
		char[] AB;
		String ABS=new String();
		Scanner sc = new Scanner(System.in);
		T=sc.nextInt();
		//while(sc.hasNext()) {
		for(int i=0;i<T;i++) {
			ABS=sc.next();
			AB=ABS.toCharArray();
			int A=AB[0]-'0';
			int B=AB[2]-'0';
			
		    System.out.println(A+B);
		}
	}
}
